{
  "name": "loki",
  "version": "1.0.0",
  "description": "An application to lock my device when I'm away",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "watch": "webpack --config webpack.common.js --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunn-e/Loki.git"
  },
  "keywords": [
    "Electron",
    "JavaScript",
    "Tensorflow",
    "React",
    "Tool"
  ],
  "author": "https://github.com/sunn-e",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sunn-e/Loki/issues"
  },
  "homepage": "https://github.com/sunn-e/Loki#readme",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@electron-forge/cli": "^6.0.0-beta.57",
    "@electron-forge/maker-deb": "^6.0.0-beta.57",
    "@electron-forge/maker-rpm": "^6.0.0-beta.57",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.57",
    "@electron-forge/maker-zip": "^6.0.0-beta.57",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "electron": "^13.0.1",
    "sass": "^1.34.0",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "loki"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
